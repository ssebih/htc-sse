import static algorea.Robot.*;
class Main {
   public static void main(String[] args) {
      remplir(5);
      transferer(5, 3);
      vider(3);
	  transferer(5, 3);
	  remplir(5);
	  transferer(5, 3);
   }
}

'5 Boucles

class Main {
   public static void main(String[] args) {
      for (int loop = 1; loop <= 1000; loop = loop + 1) {
         System.out.print.ln("Je dois respecter le Grand Sorcier.");
      }
   }
}


import static algorea.Robot.*;
class Main {
   public static void main(String[] args) {
      
   
	  for (int loop = 1; loop <= 30; loop = loop + 1) {
         System.out.print.ln("a_");
	  }
	  
	  for (int loop = 1; loop <= 21; loop = loop + 1) {
         bas();
		 gauche();
	  }
		
      
      
   }
}