import static algorea.Robot.*; 'donner des ordres au robots de faire quelques taches
 
class Main {
   public static void main(String[] args) {
      deplacer(1, 2);
      deplacer(1, 3);
      deplacer(2, 3);
	  deplacer(1, 2);
	  deplacer(3, 2);
	  deplacer(3, 2);
	  deplacer(1, 3);
	  deplacer(2, 1);
	  deplacer(2, 3);
	  deplacer(1, 2);
	  deplacer(3, 2);
	  
   }
}

